{
  "manifest_version": 3,
  "name": "清除host cache",
  "version": "1.0",
  "description": "清除host cache和刷新socket pools，A chrome extension that clears host cache and flushes socket pools",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "chrome://net-internals/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
